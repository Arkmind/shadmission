{
  "name": "transmission-custom",
  "private": true,
  "version": "0.0.0",
  "packageManager": "npm@11.4.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:ui": "npm run dev -w transmission-ui",
    "dev:monitor": "npm run dev -w transmission-monitor",
    "dev": "turbo dev --ui tui",
    "build:ui": "npm run build -w transmission-ui",
    "build:monitor": "npm run build -w transmission-monitor",
    "build": "turbo build",
    "lint": "npm run lint -w transmission-ui"
  },
  "devDependencies": {
    "turbo": "^2.3.0"
  }
}